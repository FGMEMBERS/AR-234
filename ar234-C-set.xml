<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList include="ar234-base.xml">

  <sim>

    <description>Arado Ar 234 C (with Wheels - YASim)</description>
    <author>Emmanuel BARANGER (3D/FDM)</author>

    <flight-model>yasim</flight-model>
    <aero>ar234-C-yasim</aero>

    <tags>
      <tag>arado</tag>
      <tag>high-wing</tag>
      <tag>tricycle</tag>
      <tag>retractable-gear</tag>
      <tag>four-engine</tag>
      <tag>jet</tag>
      <tag>bomber</tag>
      <tag>reconnaissance</tag>
      <tag>1940s</tag>
    </tags>
    
    <rating>
      <FDM type="int">     2 </FDM>
      <systems type="int"> 1 </systems>
      <cockpit type="int"> 3 </cockpit>
      <model type="int">   4 </model>
    </rating>

    <model>
      <path>Aircraft/AR-234/Models/ar234-C.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
    </model>
    
    <startup>
      <splash-texture>Aircraft/AR-234/Splash/ar234-C-splash.png</splash-texture>
    </startup>
    
  </sim>

</PropertyList>
