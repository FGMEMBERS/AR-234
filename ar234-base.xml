<?xml version="1.0"?>

<PropertyList>

  <sim include="Nasal/views.xml">

    <status>alpha</status>
    <author>Michael Koehne (FDM) / BARANGER Emmanuel (3D)</author>

    <fuel-fraction>1.0</fuel-fraction>

    <startup>
     <splash-texture>Aircraft/AR-234/ar234-splash.png</splash-texture>
    </startup>

    <model>
     <path>Aircraft/AR-234/Models/ar234.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
    </model>

    <sound>
      <audible>true</audible>
      <path>Aircraft/AR-234/Sounds/ar234-sound.xml</path>
    </sound>

    <virtual-cockpit archive="y">true</virtual-cockpit>

    <help>
      <title>Arado Ar 234</title>
      <key>
        <name>d</name>
        <desc>Open/Close canopy</desc>
      </key>
    </help>

    <menubar include="Gui/ar234-menu.xml"/>

  </sim>

  <engines>
    <engine n="0">
      <engine-num type="int">0</engine-num>
      <running>true</running>
    </engine>
    <engine n="1">
      <engine-num type="int">1</engine-num>
      <running>true</running>
    </engine>
  </engines>

  <consumables>
    <fuel>
      <tank n="0">
        <tank-num type="int">0</tank-num>
        <level-gal_us archive="y">1250</level-gal_us>
      </tank>
      <tank n="1">
        <tank-num type="int">1</tank-num>
        <level-gal_us archive="y">1250</level-gal_us>
      </tank>
    </fuel>
  </consumables>

  <input>
    <keyboard include="Nasal/ar234-keyboard.xml">
    </keyboard>
  </input>
 
  <controls>
   <!-- gear> <brake-parking>1</brake-parking> </gear -->
  </controls>

  <nasal>
    <doors>
      <file>Aircraft/AR-234/Nasal/doors.nas</file>
      <file>Aircraft/AR-234/Nasal/liveries.nas</file>
    </doors>
  </nasal>

</PropertyList>
