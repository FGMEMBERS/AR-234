<?xml version="1.0"?>
<PropertyList>

 <path>clock.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>hour</object-name>
  <property>/instrumentation/clock/local-hour</property>
  <factor>30</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>minute</object-name>
  <property>/instrumentation/clock/indicated-min</property>
  <factor>6</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>second</object-name>
  <property>/instrumentation/clock/indicated-sec</property>
  <factor>6</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>
