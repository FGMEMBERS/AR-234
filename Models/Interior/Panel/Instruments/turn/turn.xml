<?xml version="1.0"?>
<PropertyList>

 <path>turn.ac</path>

 <animation> 
  <type>rotate</type>
  <object-name>ball</object-name>
  <property>instrumentation/slip-skid-ball/indicated-slip-skid</property>
  <interpolation> 
    <entry> 
      <ind>-3</ind>
      <dep>-30</dep>
    </entry>
    <entry> 
      <ind>0</ind>
      <dep>0</dep>
    </entry>
    <entry> 
      <ind>3</ind>
      <dep>30</dep>
    </entry>
  </interpolation>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.02</z-m>
  </center>
  <axis> 
    <x>1</x>
    <y>0</y>
    <z>0</z>
  </axis>
 </animation>

 <animation> 
  <type>rotate</type>
  <object-name>minute</object-name>
  <property>instrumentation/turn-indicator/indicated-turn-rate</property>
  <interpolation> 
    <entry> 
      <ind>-4</ind>
      <dep>-60</dep>
    </entry>
    <entry> 
      <ind>-3</ind>
      <dep>-45</dep>
    </entry>
    <entry> 
      <ind>-2</ind>
      <dep>-30</dep>
    </entry>
    <entry> 
      <ind>-1</ind>
      <dep>-15</dep>
    </entry>
    <entry> 
      <ind>0</ind>
      <dep>0</dep>
    </entry>
    <entry> 
      <ind>1</ind>
      <dep>15</dep>
    </entry>
    <entry> 
      <ind>2</ind>
      <dep>30</dep>
    </entry>
    <entry> 
      <ind>3</ind>
      <dep>45</dep>
    </entry>
    <entry> 
      <ind>4</ind>
      <dep>60</dep>
    </entry>
  </interpolation>
  <axis> 
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
 </animation>

</PropertyList>
